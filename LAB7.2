public class loop2 {
    public static void main(String[] args) {
        int lnches = 0;
        while (lnches < 199) {
            lnches++;
            double cent = lnches * 2.54;

            System.out.println("Inches          Centimeters");
            System.out.println(lnches + "               " + cent);
        }
    }

}
